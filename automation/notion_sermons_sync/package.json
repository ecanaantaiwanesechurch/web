{
  "dependencies": {
    "@google-cloud/functions-framework": "^3.4.2",
    "@google-cloud/local-auth": "^3.0.1",
    "@notionhq/client": "^2.2.15",
    "googleapis": "^143.0.0"
  },
  "type": "module",
  "name": "notion_sermons_sync",
  "version": "0.0.1",
  "description": "Syncing Google Sheets and Notions",
  "main": "index.js",
  "devDependencies": {
    "@eslint/js": "^9.9.1",
    "eslint": "^9.9.1",
    "globals": "^15.9.0"
  },
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "test schools": "npx @google-cloud/functions-framework --target=syncSundaySchools",
    "test sermons": "npx @google-cloud/functions-framework --target=syncSermons",
    "test videos": "npx @google-cloud/functions-framework --target=syncVideos",
    "test photos": "npx @google-cloud/functions-framework --target=syncPhotos",
    "deploy schools": "gcloud functions deploy schools_sync --gen2 --runtime=nodejs22 --region=us-west1 --source=.  --entry-point=syncSundaySchools --trigger-http",
    "deploy sermons": "gcloud functions deploy sermons_sync --gen2 --runtime=nodejs22 --region=us-west1 --source=.  --entry-point=syncSermons --trigger-http",
    "deploy videos": "gcloud functions deploy videos_sync --gen2 --runtime=nodejs22 --region=us-west1 --source=.  --entry-point=syncVideos --trigger-http",
    "deploy photos": "gcloud functions deploy photos_sync --gen2 --runtime=nodejs22 --region=us-west1 --source=.  --entry-point=syncPhotos --trigger-http"
  },
  "author": "",
  "license": "ISC"
}
